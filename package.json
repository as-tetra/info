{
  "name": "as-tetra-info",
  "version": "1.0.0",
  "description": "Movable Type to Eleventy static site migration",
  "scripts": {
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-images:webp": "node scripts/optimize-images.js --webp",
    "build": "npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "clean": "rm -rf _site",
    "prebuild": "npm run clean",
    "fix-urls": "node scripts/fix-absolute-urls.js",
    "fix-urls:dry": "node scripts/fix-absolute-urls.js --dry-run",
    "fix-urls:all": "node scripts/fix-absolute-urls.js --fix-upload",
    "remove-php": "node scripts/remove-php-and-rename-index.js",
    "remove-php:dry": "node scripts/remove-php-and-rename-index.js --dry-run",
    "extract-entries": "node scripts/extract-entries.js",
    "replace-image-php": "node scripts/replace-image-php.js",
    "replace-image-php:dry": "node scripts/replace-image-php.js --dry-run"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "sharp": "^0.33.0",
    "glob": "^10.0.0",
    "fs-extra": "^11.0.0",
    "cheerio": "^1.0.0",
    "xml2js": "^0.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
