{
  "name": "as-tetra-info",
  "version": "1.0.0",
  "description": "Movable Type to Eleventy static site migration",
  "scripts": {
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-images:webp": "node scripts/optimize-images.js --webp",
    "clean": "rm -rf _site",
    "prebuild": "npm run clean",
    "build": "npx @11ty/eleventy",
    "postbuild": "node scripts/postbuild-add-prefix.js",
    "build:local": "PATH_PREFIX= npx @11ty/eleventy",
    "build:ghpages": "PATH_PREFIX=/info npx @11ty/eleventy && PATH_PREFIX=/info node scripts/postbuild-add-prefix.js",
    "build:custom": "npm run build && npm run postbuild",
    "serve": "PATH_PREFIX= npx @11ty/eleventy --serve",
    "serve:ghpages": "PATH_PREFIX=/info npx @11ty/eleventy --serve",
    "start": "npm run serve",
    "fix-urls": "node scripts/fix-absolute-urls.js",
    "fix-urls:dry": "node scripts/fix-absolute-urls.js --dry-run",
    "fix-urls:all": "node scripts/fix-absolute-urls.js --fix-upload",
    "remove-php": "node scripts/remove-php-and-rename-index.js",
    "remove-php:dry": "node scripts/remove-php-and-rename-index.js --dry-run",
    "extract-entries": "node scripts/extract-entries.js",
    "extract-genre-entries": "node scripts/extract-genre-entries.js",
    "extract-archive-entries": "node scripts/extract-archive-entries.js",
    "convert-feature-php": "node scripts/convert-feature-php-to-html.js",
    "remove-img-height": "node scripts/remove-img-height.js",
    "replace-php-links": "node scripts/replace-php-links-with-html.js",
    "replace-image-php": "node scripts/replace-image-php.js",
    "replace-image-php:dry": "node scripts/replace-image-php.js --dry-run",
    "add-path-prefix": "node scripts/add-path-prefix.js",
    "add-path-prefix:dry": "node scripts/add-path-prefix.js --dry-run",
    "remove-path-prefix": "node scripts/remove-path-prefix.js",
    "remove-path-prefix:dry": "node scripts/remove-path-prefix.js --dry-run",
    "fix-hardcoded-urls": "node scripts/fix-hardcoded-urls.js",
    "fix-hardcoded-urls:dry": "node scripts/fix-hardcoded-urls.js --dry-run",
    "fix-nfd-urls": "node scripts/fix-nfd-urls-in-html.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "sharp": "^0.33.0",
    "glob": "^10.0.0",
    "fs-extra": "^11.0.0",
    "cheerio": "^1.0.0",
    "xml2js": "^0.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
